// Testing that a lambda creates a function with a unique name for similar functions

int apply(fun f, int x) {	
	return f(x);
}

int foo(int x) {
	int a = apply((,y) -> y - 2, x); // Different function
	
	return a;
}

int moo(int x) {
	int a = apply((,y) -> y + y, x); // Different function
	
	return a;
}

harness void main(){	
	int a = foo(5);
	int b = moo(2);
	
    assert a + b == 7; 
}