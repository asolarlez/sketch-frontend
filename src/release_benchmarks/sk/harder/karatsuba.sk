int[N] plus(int[N] x, int[N] y){ /* automatically rewritten */
	int[N] out = 0;
	for(int i = 0; i<N; ++i){
		int tmp = x[i] + y[i] + out[i];
		out[i] = tmp % BASE;
		if(i < N-1){
			out[i+1] = 	tmp / BASE;
		}
	}	
	return out;
}

int[N] shiftVect(int[N] in, int s){ /* automatically rewritten */
		int[N] out = 0;
		for(int i=0; i<N; ++i){
			if(i >= s){
				out[i] = in[i-s];
			}
		}
		return out;
}

int[N] minus(int[N] t){ /* automatically rewritten */
	int[N] out = 0;
	for(int i=0; i<N; ++i){
		out[i] = -t[i];	
	}	
	return out;
}

