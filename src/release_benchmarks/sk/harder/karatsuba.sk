generator int sgn(int i){ /* automatically rewritten */
	return i >= 0 ? 1 : -1;	
}	
	
generator int[N] normalize(int[N] in){ /* automatically rewritten */
	int end = N-1;
	
	int[N] out = in;
	int s = 0;
	bit found = 0;
	for(int i = 0; i<N; ++i){
		if(!found && out[end-i]!=0){
			s = sgn(out[end-i]);
			found = 1;	
		}		
	}
	for(int i = 0; i<N-1; ++i){
		if(sgn(out[i]) != s){
			int t = BASE * s + out[i];
			out[i] = t;
			out[i+1] = out[i+1] - s; 
		}
	}		
	return out;
}



