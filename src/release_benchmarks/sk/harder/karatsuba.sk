	b = multHalf(x2, y2);
	c = multHalf(poly1(x1,x2,y1,y2), poly1(x1,x2,y1,y2));
	
	out = a;
	out = plus(out, shiftVect(b, No2));
	repeat(??){
		int[N] t = sel(sel(a, b), c);
		out = plus( out, shiftVect( sel(t, minus(t))  , isel(isel(No2, No4),0) )  );	
	}
