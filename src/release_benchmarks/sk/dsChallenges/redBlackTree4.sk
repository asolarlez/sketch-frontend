pragma options "--inlineamnt 5 --inbits 3";

int N=10;

struct node{

	int val;
	node left;
	node right;
	node father;
	bit red;	
}

struct tree{
	node root;	
}


node newNode(int v){
	node n = new node();
	n.val = v;
	n.left = null;
	n.right = null;
	n.father = null;
	n.red = 0;
	return n;
}


static bit find(tree t, int i){
	node n = t.root;
	while(n != null){
		if(n.val ==i){
			return 1;
		}
		if(n.val < i){
			n = n.right;
		}else{
			n = n.left;
		}
	}
	return 0;
}

static node tinsert(node n, node p){ // returns the inserted node.
	if(p.right != null){ 
		assert p.right.val > p.val;
		assert p.right.father == p;
		if(isRed(p.right)){ assert !p.red;  }
	}
	if(p.left != null){ 
		assert p.left.val < p.val;
		assert p.left.father == p;
		if(isRed(p.left)){ assert !p.red;  }
	}
	
	if(p.val < n.val){
		if(p.right == null){
			p.right = n;
			n.father = p;
			return n;	
		}else{
			return tinsert(n, p.right);	
		}
	}else{
		if(p.val == n.val){
			// delete n;
			return null;
		}else{
			if(p.left == null){
				p.left = n;
				n.father = p;
				return n;
			}else{
				return tinsert(n, p.left);	
			}
		}
	}	
}



void recolor( node n1, node n2, node n3){
	bit[4] c;
	c[0] = n1.red;
	c[1] = n2.red;
	c[2] = n3.red;
	c[3] = 1;
	
	n1.red = ?? ? c[??] : !c[??];
	n2.red = ?? ? c[??] : !c[??];
	n3.red = ?? ? c[??] : !c[??]; 	
}

static
node rotate1(node n1, node n2, node n3, node n4){
	return rotate(n1, n2, n3, n4);
}
static
node rotate2(node n1, node n2, node n3, node n4){	
