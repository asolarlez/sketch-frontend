		if(t2 != null){
			assert t2.val > n1.val;
		}
		if(n2!= null && n2.val < n1.val ){
			assert c2 != 3 && c2 != 5; 
		}
		if(n3!= null && n3.val < n1.val ){
			assert c2 != 6 && c2 != 8; 
		}
		if(n4!= null && n4.val < n1.val ){
			assert c2 != 9 && c2 != 11; 
